{
  "name": "pay-per-read-react",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build:watch": "babel  ./app -d build --watch --copy-files",
    "build:watch:client": "webpack --config ./webpack.config.js/ --watch ",
    "build": "babel ./app -d build --copy-files",
    "build:client": "webpack --config ./webpack.config.prod.js/",
    "build:prod": "npm run build && npm run build:client",
    "start:dev": "parallelshell 'npm run build:watch' 'npm run build:watch:client' 'nodemon --exec \" node -r dotenv/config\" ./build/server/index.js'",
    "start:prod": "npm run build:prod && NODE_ENV=production node -r dotenv/config ./build/server/index.js dotenv_config_path=./.env.prod"
  },
  "dependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "auth0-js": "^8.12.0",
    "aws-sdk": "^2.167.0",
    "axios": "^0.17.1",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.13.3",
    "contentful": "^5.0.5",
    "cookie-parser": "~1.3.5",
    "cors": "^2.8.4",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "fs": "0.0.1-security",
    "html-webpack-plugin": "^2.30.1",
    "isomorphic-unfetch": "^2.0.0",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "node-twitter-api": "^1.8.0",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "react": "^16.2.0",
    "react-css-modules": "^4.7.1",
    "react-dom": "16.2.0",
    "react-router-dom": "^4.2.2",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "style-loader": "^0.19.1",
    "twitter": "^1.7.1",
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "nodemon": "^1.12.5",
    "parallelshell": "^3.0.2"
  }
}
